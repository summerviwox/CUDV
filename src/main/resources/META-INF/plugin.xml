<idea-plugin>
    <id>com.summer.CUDV</id>
    <name>CUDV</name>
    <vendor email="summerviwox@gmail.com" url="www.summerviwox.com">summerviwox</vendor>

    <description>自动生成cudv</description>

    <depends>com.intellij.modules.platform</depends>

    <extensions defaultExtensionNs="com.intellij">
        <!-- Add your extensions here -->
    </extensions>

    <actions>
        <!-- Add your actions here -->
        <action id="com.summer.CUDVFT" class="com.summer.CUDVFT" text="CUDVFT" description="fragment自动生成cudv四个文件">
            <add-to-group group-id="ProjectViewPopupMenu" anchor="first"/>
        </action>
        <action id="com.summer.CUDVAT" class="com.summer.CUDVAT" text="CUDVAT" description="activity自动生成cudv四个文件">
            <add-to-group group-id="ProjectViewPopupMenu" anchor="first"/>
        </action>
    </actions>
</idea-plugin>